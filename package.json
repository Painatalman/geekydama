{
  "name": "geekydama",
  "version": "0.1.0",
  "description": "Dragon Ball Trivia and beer",
  "main": "dist/index.html",
  "scripts": {
    "watch": "webpack -d --watch",
    "build": "webpack --config webpack.config.prod.js",
    "build-dev": "webpack",
    "serve": "webpack-dev-server --inline",
    "start": "npm run serve",
    "open-cypress": "cypress open",
    "open-cypress-replace-snapshots": "cypress run --env updateSnapshots=true",
    "send-to-gh-pages": "git subtree push --prefix dist origin gh-pages",
    "build-and-send-to-gh-pages": "npm run build && npm run send-to-gh-pages"
  },
  "keywords": [
    "generator",
    "yeoman",
    "vue",
    "node",
    "frontend"
  ],
  "author": "Carlos Batman <maiquedi@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "cypress": "^4.1.0",
    "cypress-image-snapshot": "^3.1.1",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.1",
    "file-loader": "^5.1.0",
    "image-webpack-loader": "^6.0.0",
    "postcss": "^7.0.36",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-simple-vars": "^5.0.2",
    "style-loader": "^0.13.1",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.0",
    "vue-router": "^3.1.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.0",
    "webpack-dev-server": "^3.10.3"
  }
